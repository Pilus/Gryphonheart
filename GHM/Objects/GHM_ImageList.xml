<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ C:\Projects\WoW\Bin\Interface\FrameXML\UI.xsd">
	<CheckButton name="GHM_ImageButton_Template" checked="false" virtual="true">
        <Size>
            <AbsDimension x="74" y="74"/>
        </Size>
        <Layers>
            <Layer level="ARTWORK">
                <Texture name="$parentIconTexture">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="4" y="-4"/>
						<Anchor point="BOTTOMRIGHT" x="-4" y="4"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND">
                <FontString name="$parentAltText" justifyH="CENTER" font="Fonts\FRIZQT__.TTF" text="">
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                    <FontHeight>
                        <AbsValue val="11"/>
                    </FontHeight>
                    <Color r="1" g="1" b="1"/>
                </FontString>
            </Layer>
        </Layers>
		<Backdrop bgFile="" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<TileSize>
				<AbsValue val="16"/>
			</TileSize>
			<BackgroundInsets>
				<AbsInset left="4" right="4" top="4" bottom="4"/>
			</BackgroundInsets>
		</Backdrop>
        <PushedTexture file="Interface\Buttons\UI-Quickslot-Depress"/>
        <HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD"/>
		<CheckedTexture file="Interface\Buttons\UI-Quickslot-Depress"/>
    </CheckButton>

    <Frame name="GHM_ImageList_Template" virtual="true">
        <Size>
            <AbsDimension x="180" y="180"/>
        </Size>
		<Layers>
            <Layer level="BACKGROUND">
                <FontString name="$parentLabel" inherits="GHM_TextLabel_Template" font="GHM_GameFontSmall" text="" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT">
                            <Offset>
                                <AbsDimension x="0" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                    <FontHeight>
                        <AbsValue val="11"/>
                    </FontHeight>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
			<Frame name="$parentArea">
				<Anchors>
					<Anchor point="TOPLEFT"/>
					<Anchor point="BOTTOMRIGHT"/>
				</Anchors>
				<Backdrop bgFile="" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
					<EdgeSize>
						<AbsValue val="16"/>
					</EdgeSize>
					<TileSize>
						<AbsValue val="16"/>
					</TileSize>
					<BackgroundInsets>
						<AbsInset left="-4" right="-4" top="-4" bottom="-4"/>
					</BackgroundInsets>
				</Backdrop>
				<Frames>
					<ScrollFrame name="$parentScroll" inherits="UIPanelScrollFrameTemplate">
						<Size x="10" y="10"/>
						<Anchors>
							<Anchor point="TOPLEFT" x="5" y="-5"/>
							<Anchor point="BOTTOMRIGHT" x="-26" y="5"/>
						</Anchors>
						<ScrollChild>
							<Frame name="$parentChild">
								<Anchors>
									<Anchor point="TOPLEFT"/>
								</Anchors>
								<Size>
									<AbsDimension x="10" y="10"/>
								</Size>
							</Frame>
						</ScrollChild>
					</ScrollFrame>
				</Frames>
			</Frame>
        </Frames>
    </Frame>
</Ui>